#include "StateContext.h"

// Used to clean up adding transitions
#define T(a) (t##a)

StateContext::StateContext()
{
	threadAlive = true;
	Transition* t0 = new Transition(RUN_CYCLOMETER, NO_SIGNAL); // reset to start
	Transition* t1 = new Transition(FULL_RESET, ALL_HELD); // start to reset
}

StateContext::~StateContext(){}
