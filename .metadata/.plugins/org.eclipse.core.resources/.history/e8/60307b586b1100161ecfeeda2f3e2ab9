#ifndef SIGNALS_H_
#define SIGNALS_H_

enum ButtonSignals
{
	MODE            = 1,
	SET             = 2,
	START           = 3,
	MODE_HELD       = 4,
	MODE_RELEASED   = 5,
	MODE_START_HELD = 6,
	ALL_HELD        = 7
};

enum ModeSignals
{
	CHANGE_TIRE_CIRC = 1,
	CHANGE_CALC_MODE = 2,
	STOP_POLLING     = 3,
	START_POLLING    = 4,
	PULSE            = 5,
	STOP_WATCHDOG    = 6,
	START_WATCHDOG   = 7,
	STROKE           = 8,
	TIMEOUT          = 9
};

#endif /* SIGNALS_H_ */
